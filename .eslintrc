{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-underscore-dangle": ["error", { "allowAfterThis": true }],
    "no-plusplus": [0],
    # disabled per https://github.com/benmosher/eslint-plugin-import/issues/544#issuecomment-244976007
    "import/no-named-as-default": [0],
    "react/forbid-prop-types": [0]
  }
}
